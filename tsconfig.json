{
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "extends": "./node_modules/adonis-preset-ts/tsconfig",
  "compilerOptions": {
    "paths": {
      "App/*": [
        "./app/*"
      ],
      "Contracts/*": [
        "./contracts/*"
      ]
    },
    "types": [
      "@adonisjs/bodyparser",
      "@adonisjs/session",
      "@adonisjs/view",
      "@adonisjs/core"
    ]
  }
}